# Sample logging configuration, copy to ~/.airpnprc to use

[loggers]
keys=root,bridgeserver

[handlers]
keys=h1

[formatters]
keys=f1

[logger_root]
level=INFO
handlers=h1

[logger_bridgeserver]
level=DEBUG
handlers=h1
qualname=airpnp.bridge-server
propagate=0

[handler_h1]
class=StreamHandler
level=NOTSET
formatter=f1
args=(sys.stdout,)

[formatter_f1]
format=[%(asctime)s %(levelname)s] %(message)s
datefmt=
class=logging.Formatter

